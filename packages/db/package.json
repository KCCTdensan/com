{
  "name": "db",
  "dependencies": {
    "@prisma/client": "4.8.1"
  },
  "devDependencies": {
    "prisma": "4.8.1",
    "tsup": "6.5.0",
    "typescript": "4.9.4"
  },
  "files": [
    "dist/**"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "private": true,
  "scripts": {
    "build": "tsup --minify",
    "dev": "tsup --watch",
    "format": "prisma format",
    "generate": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "prebuild": "npm run generate",
    "predev": "npm run generate",
    "push": "prisma db push",
    "studio": "prisma studio"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "sourcemap": true
  },
  "types": "./dist/index.d.ts"
}
