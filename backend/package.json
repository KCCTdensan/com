{
  "dependencies": {
    "@fastify/cors": "8.2.0",
    "@fastify/sensible": "5.2.0",
    "@fastify/static": "6.6.0",
    "@swc/core": "1.3.22",
    "cross-env": "7.0.3",
    "fastify": "4.10.2",
    "fastify-socket.io": "4.0.0",
    "sanitize-html": "2.8.0",
    "socket.io": "4.5.4",
    "ts-node": "10.9.1"
  },
  "devDependencies": {
    "@prisma/client": "4.7.1",
    "@types/node": "~18.11.13",
    "@types/sanitize-html": "2.6.2",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "prisma": "4.7.1",
    "typescript": "~4.9.4"
  },
  "nodemonConfig": {
    "ext": "ts",
    "exec": "ts-node-esm"
  },
  "private": true,
  "scripts": {
    "check": "run-s gen tsc",
    "dev": "run-p dev:*",
    "dev:node": "cross-env NODE_ENV=development nodemon --experimental-specifier-resolution=node src",
    "dev:prisma": "prisma studio",
    "gen": "prisma generate",
    "start": "cross-env NODE_ENV=production ts-node-esm --experimental-specifier-resolution=node src",
    "tsc": "tsc --noEmit"
  },
  "type": "module"
}
