{
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@remix-run/express": "^1.7.6",
    "@remix-run/node": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "@socket.io/redis-adapter": "^7.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-twemoji": "^0.5.0",
    "redis": "^4.5.1",
    "sanitize-html": "^2.7.3",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@prisma/client": "^4.6.1",
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/sanitize-html": "^2.6.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prisma": "^4.6.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=14"
  },
  "nodemonConfig": {
    "watch": "server",
    "ext": "ts",
    "exec": "ts-node"
  },
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-s generate:css build:remix && run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:node": "cross-env NODE_ENV=development nodemon server",
    "dev:remix": "remix watch",
    "dev:studio": "prisma studio",
    "generate:css": "tailwindcss -i app/styles/main.css -o app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location .cache/eslint .",
    "start": "cross-env NODE_ENV=production ts-node --transpile-only server"
  },
  "sideEffects": false
}
